{
	"manifest_version": 2,
	"name": "Hexconverter",
	"version": "1.1.1",
	"author": "Lionel Mahistre",

	"description": "Decimal, hexadecimal, binary and octal converter",
	"icons": {
		"32": "hexc-32.png",
		"48": "hexc-48.png",
